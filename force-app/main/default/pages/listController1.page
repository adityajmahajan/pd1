<apex:page standardController="Account" recordSetVar="accts">
    <apex:pageBlock title="Accounts">
        <apex:pageBlockTable value="{!accts}" var="a">
            <apex:column value="{!a.Name}"/>
            <apex:column value="{!a.AccountNumber}"/>        
        </apex:pageBlockTable>
    </apex:pageBlock>
</apex:page>